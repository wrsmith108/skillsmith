{
  "name": "@smith-horn/enterprise",
  "version": "0.1.2",
  "description": "Enterprise features for Skillsmith including audit logging, SSO, and RBAC",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./audit": {
      "types": "./dist/src/audit/index.d.ts",
      "import": "./dist/src/audit/index.js"
    },
    "./audit/streaming": {
      "types": "./dist/src/audit/streaming/EventStream.d.ts",
      "import": "./dist/src/audit/streaming/EventStream.js"
    },
    "./license": {
      "types": "./dist/src/license/index.d.ts",
      "import": "./dist/src/license/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.980.0",
    "@skillsmith/core": "0.4.1",
    "jose": "5.10.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@smithy/config-resolver": "4.4.6",
    "@smithy/core": "3.5.3",
    "@smithy/middleware-endpoint": "4.1.10",
    "@smithy/middleware-retry": "4.1.11",
    "@smithy/protocol-http": "5.1.2",
    "@smithy/smithy-client": "4.4.3",
    "@smithy/types": "4.3.1",
    "@smithy/util-retry": "4.1.2",
    "vitest": "4.0.16"
  },
  "files": [
    "dist"
  ],
  "license": "Elastic-2.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smith-horn/skillsmith.git",
    "directory": "packages/enterprise"
  },
  "homepage": "https://skillsmith.dev/enterprise",
  "bugs": {
    "url": "https://github.com/smith-horn/skillsmith/issues",
    "email": "enterprise@skillsmith.dev"
  },
  "keywords": [
    "claude",
    "ai",
    "skills",
    "mcp",
    "enterprise",
    "sso",
    "rbac",
    "audit"
  ],
  "engines": {
    "node": ">=22.0.0"
  }
}
